{
	"manifest_version": 3,
	"name": "TextWork",
	"description": "Описание расширения",
	"version": "0.0.1",
	"author": "Автор",
	"action": {
		"default_title": "Работа с текстом",
		"default_icon": "icon.png",
		"default_popup": "main.html"
	},
	"content_scripts": [
		{
			"matches":[
				"<all_urls>"
			],
			"js": ["script.js", "requestWiki.js"]
		}
	],
	"permissions": ["storage", "activeTab", "scripting"]
}